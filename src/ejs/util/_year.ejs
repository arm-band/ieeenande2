<%
//コピーライトの部分に出す年数を生成
funcCopyRightYear = (variables) => {
    var copyRightYear = "";
    var getFullYear = new Date().getFullYear();
    if(parseInt(getFullYear) - parseInt(variables.commons.year) === 0) {
        copyRightYear = variables.commons.year;
    }
    else {
        copyRightYear = `${variables.commons.year}-${getFullYear}`;
    }
    return copyRightYear;
}
%>