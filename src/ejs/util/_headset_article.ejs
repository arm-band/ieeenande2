<%
//headタグ内のmeta属性のうち、titleとdescriptionとomgimageを判別
metaTitle = config.param[id].title; //グローバル変数
metaDescription = config.param[id].description;
metaOgpimage = `${config.commons.url}img/${config.param[id].ogpimage}`;
//タイトル
if(attributes.title.length > 0) {
    metaTitle = attributes.title;
}
//説明
if(attributes.excerpt.length > 0) {
    metaDescription = attributes.excerpt;
}
//OGP画像
if(attributes.thumbnail !== null && attributes.thumbnail !== undefined) {
    metaOgpimage = `${config.commons.url}img/${attributes.thumbnail}`;
}
%>