<%
//ドメイン
domain = variables.commons.url.slice(0, -1) + variables.commons.baseurl;
//headタグ内のmeta属性のうち、titleとdescriptionとomgimageを判別
metaTitle = variables.param[id].title; //グローバル変数
metaDescription = variables.param[id].description;
metaOgpimage = domain + "img/" + variables.param[id].ogpimage;
//タイトル
if(newsBlock.title.length > 0) {
    metaTitle = newsBlock.title;
}
//説明
if(newsBlock.excerpt.length > 0) {
    metaDescription = newsBlock.excerpt;
}
//OGP画像
if(newsBlock.thumbnail.length > 0) {
    metaOgpimage = domain + "img/" + newsBlock.thumbnail;
}
%>