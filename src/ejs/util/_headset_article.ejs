<%
//headタグ内のmeta属性のうち、titleとdescriptionとomgimageを判別
metaTitle = config.param[id].title; //グローバル変数
metaDescription = config.param[id].description;
metaOgpimage = `${config.commons.url}img/${config.param[id].ogpimage}`;
//タイトル
if(newsBlock.title.length > 0) {
    metaTitle = newsBlock.title;
}
//説明
if(newsBlock.excerpt.length > 0) {
    metaDescription = newsBlock.excerpt;
}
//OGP画像
if(newsBlock.thumbnail.length > 0) {
    metaOgpimage = `${config.commons.url}img/${newsBlock.thumbnail}`;
}
%>