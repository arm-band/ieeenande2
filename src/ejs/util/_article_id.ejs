<%
//記事ページのURLを生成
funcArticleUrl = (url, attributes) => {
    var urlTitle = attributes.title;
    urlTitle = urlTitle.replace(/\./g, "_");
    var datetime = funcDateTimeFormat(attributes.date, "", "ymd");
    var fileUrl = `releasenote_${urlTitle}-${datetime}`;
    var articleIdUrl = `${url}news/articles/${fileUrl}.html`;
    return articleIdUrl;
}
%>