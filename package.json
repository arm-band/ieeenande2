{
  "name": "kiribi_ususama",
  "version": "3.8.0-4.1.3",
  "description": "gulpを使用した小・中規模Webサイト制作用のボイラープレートです。",
  "main": "gulpfile.js",
  "scripts": {
    "kashou": "mkdirp dist sg/dist src/scss/assets src/scss/assets/bootstrap src/scss/assets/bootstrap/honoka src/scss/assets/bootstrap/honoka/bootstrap src/scss/assets/bootstrap/honoka/honoka src/scss/assets/bootstrap/honoka/bootstrap/scss src/scss/assets/bootstrap/honoka/bootstrap/scss/mixins src/scss/assets/bootstrap/honoka/bootstrap/scss/utilities",
    "zanmai": "copyfiles -f \"./node_modules/bootstrap-honoka/scss/*.scss\" \"./src/scss/assets/bootstrap\"",
    "gueshingon": "copyfiles -f \"./node_modules/bootstrap-honoka/scss/honoka/_honoka.scss\" \"./src/scss/assets/bootstrap/honoka\"",
    "om": "copyfiles -f \"./node_modules/bootstrap-honoka/scss/honoka/*.scss\" \"./src/scss/assets/bootstrap/honoka/honoka\"",
    "krodhana": "rename-files bootstrap.scss _bootstrap.scss --PATH=./src/scss/assets/bootstrap/",
    "hum": "copyfiles -f \"./node_modules/bootstrap/scss/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss\"",
    "jah": "copyfiles -f \"./node_modules/bootstrap/scss/mixins/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss/mixins\"",
    "svaha": "copyfiles -f \"./node_modules/bootstrap/scss/utilities/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss/utilities\"",
    "ususama": "npm-run-all -s kashou zanmai gueshingon om krodhana hum jah svaha",
    "gulp": "gulp",
    "kiribi": "node bin/console",
    "fujoketsu": "rimraf ./src/scss/assets/ ./src/js/concat/ ./src/scss/util/_var.scss ./dist/ ./sg/dist/ ./package-lock.json"
  },
  "author": "Arm Band",
  "license": "ISC",
  "devDependencies": {
    "brand-colors": "^2.0.1",
    "bootstrap": "^4.1.3",
    "bootstrap-honoka": "^4.1.3",
    "bowser": "^1.9.4",
    "browser-sync": "^2.24.4",
    "copyfiles": "^2.0.0",
    "gulp": "next",
    "gulp-concat": "^2.6.1",
    "gulp-connect-php": "^1.0.3",
    "gulp-data": "^1.3.1",
    "gulp-ejs": "^3.1.3",
    "gulp-frontnote": "^2.0.1",
    "gulp-imagemin": "^4.1.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^4.0.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-uglify": "^3.0.0",
    "gulp-watch": "^5.0.1",
    "jquery": "^3.3.1",
    "jquery.easing": "^1.4.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "readline-sync": "^1.4.9",
    "rename-files-cli": "^1.0.1",
    "rimraf": "^2.6.2",
    "rss": "^1.2.2",
    "yaml": "1.0.0-rc.8"
  }
}
