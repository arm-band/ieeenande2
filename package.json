  "main": "gulpfile.js",
  "scripts": {
    "proinit:mkdir": "mkdirp dist sg/dist src/scss/assets src/scss/assets/bootstrap src/scss/assets/bootstrap/honoka src/scss/assets/bootstrap/honoka/bootstrap src/scss/assets/bootstrap/honoka/honoka src/scss/assets/bootstrap/honoka/bootstrap/scss src/scss/assets/bootstrap/honoka/bootstrap/scss/mixins src/scss/assets/bootstrap/honoka/bootstrap/scss/utilities",
    "proinit:honoka1": "copyfiles -f \"./node_modules/bootstrap-honoka/scss/*.scss\" \"./src/scss/assets/bootstrap\"",
    "proinit:honoka2": "copyfiles -f \"./node_modules/bootstrap-honoka/scss/honoka/_honoka.scss\" \"./src/scss/assets/bootstrap/honoka\"",
    "proinit:honoka3": "copyfiles -f \"./node_modules/bootstrap-honoka/scss/honoka/*.scss\" \"./src/scss/assets/bootstrap/honoka/honoka\"",
    "proinit:honoka4": "rename-files bootstrap.scss _bootstrap.scss --PATH=./src/scss/assets/bootstrap/",
    "proinit:bootstrap1": "copyfiles -f \"./node_modules/bootstrap/scss/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss\"",
    "proinit:bootstrap2": "copyfiles -f \"./node_modules/bootstrap/scss/mixins/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss/mixins\"",
    "proinit:bootstrap3": "copyfiles -f \"./node_modules/bootstrap/scss/utilities/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss/utilities\"",
    "proinit": "npm-run-all -s proinit:mkdir proinit:honoka1 proinit:honoka2 proinit:honoka3 proinit:honoka4 proinit:bootstrap1 proinit:bootstrap2 proinit:bootstrap3",
    "kashou": "npm run proinit:mkdir",
    "zanmai": "npm run proinit:honoka1",
    "gueshingon": "npm run proinit:honoka2",
    "om": "npm run proinit:honoka3",
    "krodhana": "npm run proinit:honoka4",
    "hum": "npm run proinit:bootstrap1",
    "jah": "npm run proinit:bootstrap2",
    "svaha": "npm run proinit:bootstrap3",
    "ususama": "npm-run-all -s kashou zanmai gueshingon om krodhana hum jah svaha",
    "gulp": "gulp",
    "kiribi": "node bin/console"
  },
  "author": "Arm Band",
  "license": "ISC",
